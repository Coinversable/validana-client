{
	"compilerOptions": {
		"types": [
			"node"
		],
		"target": "es5",
		"lib": [
			"dom",
			"es6"
		],
		"module": "commonjs",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"inlineSourceMap": true,
		"strictNullChecks": true,
		"strictPropertyInitialization": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"outDir": "./dist"
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}